<div id="iMAIN">
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href={{ asset('css/style.css') }}>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

</head>
<body>
<div class="container.fluid">
    <div class="flex-row" id="bandeau">
        <a href="{{ path('home') }}"><img id="bandeau-image" src="{{ asset('images/bandeau.jpg') }}"></a>
    </div>
    <div class="d-flex" id="wrapper">
    <div class="bg-light border-right" id="sidebar-wrapper">
        <div class="sidebar-heading">Menu </div>
        <div class="list-group list-group-flush">
            <a href="{{ path('home') }}" class="list-group-item list-group-item-action bg-light" id="">Accueil</a>
            <a href="{{ path('catalogue') }}" class="list-group-item list-group-item-action bg-light" id="">Catalogue</a>
            <a href="{{ path('Gestion.Auteurs.index') }}" class="list-group-item list-group-item-action bg-light" id="">Gestion Auteur</a>
            <a href="{{ path('Gestion.Themes.index') }}" class="list-group-item list-group-item-action bg-light" id="">Gestion Theme</a>
            <a href="#" class="list-group-item list-group-item-action bg-light" id="">Item 5</a>
            <a href="#" class="list-group-item list-group-item-action bg-light" id="">Item 6</a>
        </div>
    </div>

{% block body %}{% endblock %}
    </div>
</div>
{% block javascripts %}{% endblock %}
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
</body>
</html>
</div>
